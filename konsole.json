{
  "es": {
    "timefield": "received_at",
    "default_index": "logstash-*",
    "allow_url_parameter": false,
    "timezone": "UTC"
  },
  "max_buckets": 100,
  "fields" : [
    {
      "type" : "syslog",
      "fields": ["received_at","syslog_timestamp", "syslog_hostname", "syslog_program", "syslog_message"]
    }
  ]
}
